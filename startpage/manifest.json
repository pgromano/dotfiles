{
    "manifest_version": 2,
    "name": "Custom New Tab",
    "version": "0.0.0",
    "description": "Custom homepage for new tabs and windows",
    "chrome_url_overrides": {
      "newtab": "startpage.html"
    },
    "content_security_policy": "script-src 'self'; script-src 'unsafe-inline'; object-src 'self';"
}